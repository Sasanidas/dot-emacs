# -*- mode: snippet -*-
# name: fori
# key: fori
# --
for (${1:auto it} = ${2:var}.begin(); ${1:$(string-trim (if (string-match "[^ ]+" yas-text) (replace-match "" t t yas-text) yas-text))} != $2.end(); ++${1:$(string-trim (if (string-match "[^ ]+" yas-text) (replace-match "" t t yas-text) yas-text))})
{
    $0
}